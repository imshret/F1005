
@{
    ViewBag.Title = "Now";
}


<h2>目前匯率</h2> 
<button type="button">
    <img class="ui-icon-arrow-1-s" src="data:image/svg+xml;utf8;base64," />


    @Html.ActionLink("匯率轉換", "ExchangeRates", "FX")
</button>
<div style="background-color:lightblue;">
    <table id="myTable" class="table table-striped table-bordered" style="width:100%"></table>
</div>
<div style="height:200px"></div>

@section styles
{
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/DataTables/css/jquery.dataTables.min.css" rel="stylesheet" />

    <link href="~/Areas/Main/Smartmenus/sm-core-css.css" rel="stylesheet" />
    <link href="~/Areas/Main/Smartmenus/sm-blue.css" rel="stylesheet" />
}
@section scripts
{
    <script src="~/Scripts/jquery-3.3.1.min.js"></script>
    <script src="~/Scripts/DataTables/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>

    @*NavBar*@
    <script src="~/Areas/Main/Smartmenus/jquery.smartmenus.js"></script>

    <script>
        function LoadData() {
            var datatable = $('#myTable').DataTable({
                @*ajax: "@Url.Action("Getlist", "Money", new { Area = "ForeignExchange" }, null)",*@
                "ajax": {"url":"@Url.Action("Getlist", "Money", new { Area = "ForeignExchange" }, null)","dataSrc":""},
                columns: [
                    { title: '中文', data: "CurrencyClassName" },
                    { title: '外幣縮寫', data: "CurrencyClass" }, 
                    { title: '即期買入', data: "OnlineBuy" },
                    { title: '即期賣出', data: "OnlineSell" },
                    //{ title: '更新日期', data: "Date" },
                ]
            });
        }
        $(function () {
            LoadData();
        });
    </script>
}